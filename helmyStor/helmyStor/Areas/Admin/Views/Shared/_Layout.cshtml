                    <ul class="menu-content">
                        <li><a asp-area="Admin" asp-controller="Products" asp-action="Create"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Shop">Create Product</span></a>
                        </li>                                  
                        <li><a asp-area="Admin" asp-controller="Products" asp-action="List"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Details">All Products</span></a>
                        </li>                                   

                    </ul>
                </li>
                 <li class=" nav-item"><a href="#"><i class="feather icon-unlock"></i><span class="menu-title" data-i18n="Authentication">Authentication</span></a>
                    <ul class="menu-content">
                        <li><a asp-area="Admin" asp-controller="Adminstraition" asp-action="CreateRole"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Card">Create Role</span></a>
                        </li>                                   
                        <li><a asp-area="Admin" asp-controller="Adminstraition" asp-action="ListRoles"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Card">All Roles</span></a>
                        </li>

                    </ul>
                </li>
                <li class=" nav-item"><a href="#"><i class="feather icon-list"></i><span class="menu-title" data-i18n="Data List">Category</span></a>
                    <ul class="menu-content">
                        <li><a asp-area="Admin" asp-controller="Category" asp-action="Create"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="List View">Create Category</span></a>
                        </li>
                        <li><a asp-area="Admin" asp-controller="Category" asp-action="List"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Thumb View">All Categories</span></a>
                        </li>
                    </ul>
                </li>

                <li class=" nav-item"><a href="#"><i class="feather icon-eye"></i><span class="menu-title" data-i18n="Icons">Sliders</span></a>
                    <ul class="menu-content">
                        <li><a asp-area="Admin" asp-controller="Slider" asp-action="Create"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Feather">Create Slider</span></a>
                        </li>
                        <li><a asp-area="Admin" asp-controller="Slider" asp-action="List">><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Font Awesome">All Sliders</span></a>
                        </li>
                    </ul>
                </li>
                <li class=" nav-item"><a href="#"><i class="feather icon-credit-card"></i><span class="menu-title" data-i18n="Card">Orders</span></a>
                    <ul class="menu-content">
                        <li><a asp-area="Admin" asp-controller="Invoices" asp-action="List"><i class="feather icon-circle"></i><span class="menu-item" data-i18n="Basic">All Orders</span></a>
                        </li>
                    </ul>
                </li>

            </ul>
        </div>
    </div>
    <!-- END: Main Menu-->
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
